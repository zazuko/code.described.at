@base <https://code.described.at/> .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <http://schema.org/> .

<>
  schema:hasPart
    <EcmaScript>,
    <EcmaScriptTemplateLiteral>,
    <SparqlEcmaScriptTemplateLiteral>,
    <Variable>,
    <VariableName>,
    <arguments>,
    <implementedBy>,
    <link>,
    <name>,
    <value>,
    <variable>,
    <variables>.

<EcmaScript> a rdfs:Class;
  rdfs:label "ECMAScript Code";
  rdfs:comment "Code as defined by the ECMAScript specification".

<EcmaScriptTemplateLiteral> a rdfs:Class;
  rdfs:label "ECMAScript Template Literal";
  rdfs:comment "Template Literal as defined in the ES2015 ECMAScript specification".

<SparqlEcmaScriptTemplateLiteral> a rdfs:Class;
  rdfs:label "SPARQL ECMAScript Template Literal";
  rdfs:comment "ES2015 ECMAScript Template Literal enriched SPARQL query".

<Variable> a rdfs:Class;
  rdfs:label "Variable";
  rdfs:comment "Definition of a variable".

<VariableName> a rdfs:Class;
  rdfs:label "Variable Name";
  rdfs:comment "Name of a variable".

<arguments> a rdf:Property;
  rdfs:label "Arguments";
  rdfs:comment "Arguments for a code call".

<implementedBy> a rdf:Property;
  rdfs:label "Implementation";
  rdfs:comment "Description of the implementation".

<link> a rdf:Property;
  rdfs:label "Code";
  rdfs:comment "IRI of the executable code".

<name> a rdf:Property;
  rdfs:label "Name";
  rdfs:comment "Name of a variable".

<value> a rdf:Property;
  rdfs:label "Value";
  rdfs:comment "Value of a variable".

<variable> a rdf:Property;
  rdfs:label "Variable";
  rdfs:comment "Definition of a variable".

<variables> a rdf:Property;
  rdfs:label "Variable definitions";
  rdfs:comment "A set of variable definitions".